# editorconfig.org
root = true

[*]
indent_style = tab
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
word_wrap = false

[*.md]
trim_trailing_whitespace = true


[*.yaml]
indent_style = space
trim_trailing_whitespace = true


[*.yml]
indent_style = space
trim_trailing_whitespace = true
